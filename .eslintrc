{
    "extends": "eslint:recommended",
    "env": {
        "node": true
    },
    "rules": {
        "accessor-pairs": 0,
        "no-array-constructor": 2,
        "block-scoped-var": 2,
        "brace-style": [2, "1tbs"],
        "camelcase": 0,
        "comma-dangle": [2, "never"],
        "comma-style": 2,
        "consistent-return": 0,
        "consistent-this": 2,
        "default-case": 2,
        "dot-notation": 2,
        "eol-last": 0,
        "func-style": [2, "declaration"],
        "guard-for-in": 2,
        "handle-callback-err": 0,
        "keyword-spacing": 0,
        "no-catch-shadow": 2,
        "no-cond-assign": 2,
        "no-console": 0,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-empty-character-class": 2,
        "no-eq-null": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implicit-coercion": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-invalid-this": 2,
        "no-irregular-whitespace": 2,
        "no-lonely-if": 2,
        "no-mixed-requires": [2, true],
        "no-mixed-spaces-and-tabs": 2,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 2,
        "no-new-require": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-path-concat": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-self-compare": 2,
        "no-shadow": 0,
        "no-sparse-arrays": 2,
        "no-throw-literal": 2,
        "no-trailing-spaces": 2,
        "no-underscore-dangle": 0,
        "no-undef": 2,
        "no-undefined": 1,
        "no-unexpected-multiline": 2,
        "no-unneeded-ternary": 2,
        "no-unreachable": 2,
        "no-unused-vars": 2,
        "no-useless-call": 2,
        "no-useless-escape": 0,
        "no-useless-return": 2,
        "no-void": 2,
        "quote-props": [2, "as-needed"],
        "quotes": [2, "single"],
        "radix": 2,
        "space-after-keywords": 0,
        "space-unary-ops": [2, { "words": true, "nonwords": false }],
        "semi": [2, "always"],
        "use-isnan": 2,
        "valid-jsdoc": [0, {
            "requireReturn": false
        }],
        "valid-typeof": 2,
        "wrap-iife": 2
    },
    "globals": {
        "afterEach": false,
        "beforeEach": false,
        "expect": false,
        "describe": false,
        "fdescribe": false,
        "fit": false,
        "jasmine": false,
        "it": false,
        "spyOn": false,
        "xit": false
    }
}
